html
	include partials/_lib.pug
	.list-group
		each val in res
			form.form-inline(action='/addtolist', method='post')
				input(type='hidden', name='vidid', value= val.id.videoId)
				#thebut.list-group-item= val.snippet.title
					select.form-control(name='room')
						each val in myrooms
							option(value=val._id)= val.name 
					button.btn.btn-success(type='submit') Submit
